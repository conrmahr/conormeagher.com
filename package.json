{
  "name": "11ty-portfolio",
  "version": "1.0.0-rc3",
  "description": "Minimal Poftfolio / blog template built with eleventy & tailwindcss.",
  "scripts": {
    "serve": "ELEVENTY_ENV=dev npx eleventy --serve",
    "start": "npm run serve",
    "build": "ELEVENTY_ENV=dev npx eleventy",
    "build:prod": "ELEVENTY_ENV=prod npx eleventy",
    "build:css": "tailwindcss -i src/css/tailwind.css -o src/css/styles.css --postcss --minify"
  },
  "keywords": [
    "11ty",
    "tailwindcss",
    "eleventy",
    "purgecss"
  ],
  "author": "kailoon",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-link-attributes": "^4.0.0",
    "nunjucks": "^3.2.3",
    "tailwindcss": "^3.3.1"
  },
  "main": ".eleventy.js",
  "dependencies": {
    "@11ty/eleventy-upgrade-help": "^3.0.1",
    "eleventy-plugin-embed-everything": "^1.15.1",
    "luxon": "^3.3.0",
    "markdown-it": "^14.1.0",
    "markdown-it-footnote": "^4.0.0",
    "postcss-cli": "^11.0.0"
  }
}
